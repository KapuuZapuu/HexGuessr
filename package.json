{
    "rewrites": [
        { "source": "/unlimited",  "destination": "/" },
        { "source": "/unlimited/", "destination": "/" }
    ],
    "headers": [
        {
            "source": "/(app|styles).(js|css)",
            "headers": [
                { "key": "Cache-Control", "value": "no-cache" }
            ]
        },
        {
            "source": "/favicon.(ico|png)",
            "headers": [
                { "key": "Cache-Control", "value": "no-cache" }
            ]
        },
        {
            "source": "/",
            "headers": [
                { "key": "Cache-Control", "value": "no-cache" }
            ]
        },
        {
            "source": "/index.html",
            "headers": [
                { "key": "Cache-Control", "value": "no-cache" }
            ]
        }
    ]
}